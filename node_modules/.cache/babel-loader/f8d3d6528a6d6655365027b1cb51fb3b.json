AhB,CAAT;AAAA,KAHX;AAIH;AA1BU,CAAf","sourcesContent":["<template>\r\n    <div class=\"cities\">\r\n        <div class=\"container\">\r\n            <h2>List of cities</h2>\r\n            <button type=\"button\" class=\"add\">\r\n                <span class=\"button_text\">Add City</span>\r\n                <span class=\"button_icon\"\r\n                    ><img src=\"../assets/add.svg\" alt=\"\"\r\n                /></span>\r\n            </button>\r\n        </div>\r\n\r\n        <ul>\r\n            <li\r\n                v-for=\"item in cities\"\r\n                :key=\"item.Name\"\r\n                class=\"cities_name\"\r\n                v-on:click=\"ShowDetails\"\r\n                @click=\"this.cityName = item.Name\"\r\n            >\r\n                {{ item.Name }}\r\n            </li>\r\n        </ul>\r\n    </div>\r\n    <div class=\"cities_details\" v-if=\"showDetail\">\r\n        <CityDetails :cityData=\"cityName\" />\r\n        <p class=\"off\" v-on:click=\"ShowDetails\">X</p>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nimport CityDetails from \"../components/CityDetails.vue\";\r\n\r\nexport default {\r\n    name: \"cities\",\r\n    data() {\r\n        return {\r\n            cities: [],\r\n            showDetail: false,\r\n            cityName: \"Warszawa\",\r\n        };\r\n    },\r\n    components: {\r\n        CityDetails,\r\n    },\r\n    methods: {\r\n        ShowDetails() {\r\n            if (this.showDetail) {\r\n                this.showDetail = false;\r\n            } else {\r\n                this.showDetail = true;\r\n            }\r\n        },\r\n    },\r\n    mounted() {\r\n        fetch(\"http://34.116.152.0/Cities\")\r\n            .then((res) => res.json())\r\n            .then((data) => (this.cities = data.Data))\r\n            .catch((err) => console.log(err.message));\r\n    },\r\n};\r\n</script>\r\n\r\n<!-- Add \"scoped\" attribute to limit CSS to this component only -->\r\n<style scoped>\r\n.cities {\r\n    width: 60%;\r\n