=\"show\">\r\n            <p class=\"more\" v-on:click=\"showMoreLess('more')\">show more</p>\r\n            <p\r\n                class=\"less\"\r\n                v-if=\"this.showNo >= 10\"\r\n                v-on:click=\"showMoreLess('less')\"\r\n            >\r\n                show less\r\n            </p>\r\n        </div>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n    name: \"archive\",\r\n    data() {\r\n        return {\r\n            archiveTemp: [],\r\n            showNo: 5,\r\n        };\r\n    },\r\n    methods: {\r\n        fetchData() {\r\n            fetch(\"http://34.116.152.0/Measurments/\" + this.showNo)\r\n                .then((res) => res.json())\r\n                .then((data) => (this.archiveTemp = data.Measurments))\r\n                .catch((err) => console.log(err.message));\r\n        },\r\n        showMoreLess(choose) {\r\n            if (choose == \"more\") this.showNo += 5;\r\n            else if (choose == \"less\") this.showNo -= 5;\r\n            this.fetchData();\r\n        },\r\n    },\r\n    mounted() {\r\n        this.fetchData();\r\n    },\r\n};\r\n</script>\r\n\r\n<!-- Add \"scoped\" attribute to limit CSS to this component only -->\r\n<style scoped>\r\n* {\r\n    font-family: Open Sans;\r\n}\r\n\r\nh2 {\r\n    font-family: Montserrat;\r\n    font-weight: 700;\r\n    color: #8fbbaf;\r\n    font-size: 35px;\r\n}\r\n\r\ntd,\r\nth {\r\n    padding: 10px;\r\n    text-align: center;\r\n}\r\n\r\nth {\r\n    background-color: #8fbbaf;\r\n}\r\n\r\ntable,\r\nh2 {\r\n    width: 70%;\r\n    margin: 30px auto 0 auto;\r\n}\r\n\r\ntable {\r\n    border-collapse: collapse;\r\n}\r\ntable tr:nth-child(even) {\r\n    background-color: #f4f9f4;\r\n}\r\n\r\n.show {\r\n    width: 100%;\r\n    display: flex;\r\n    flex-wrap: wrap;\r\n    flex-direction: row;\r\n    justify-content: center;\r\n    align-items: center;\r\n\r\n    padding: 30px 0 40px 0;\r\n}\r\n\r\n.show p {\r\n    font-size: 14px;\r\n\r\n    color: black;\r\n    cursor: pointer;\r\n    transition: ease all 0.4s;\r\n}\r\n\r\n.show p:hover {\r\n    color: #c4e3cb;\r\n}\r\n\r\n.more {\r\n    margin-right: 40px;\r\n}\r\n\r\n@media screen and (max-width: 400px) {\r\n    .show {\r\n        margin-bottom: 10px;\r\n    }\r\n}\r\n</style>\r\n"],"sourceRoot":""}]}